{
  "expo": {
    "scheme": "onekey-wallet",
    "name": "OneKey Wallet",
    "slug": "OneKey-Wallet",
    "owner": "onekeyhq",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.github.longwallet.app",
      "associatedDomains": ["applinks:app.longwallet.org"]
    },
    "android": {
      "googleServicesFile": "./android/app/google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "cbdffa9d-4217-43b4-aec7-c2961f4821e6"
      }
    }
  }
}
